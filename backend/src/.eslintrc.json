{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {      
        "linebreak-style":0,     
        "no-unused-vars": 0,             
        "max-len": 0 ,
        "semi" : 0,
       "object-curly-newline":0,
        "class-methods-use-this":0,
        "global-require":0,
        "import/no-unresolved" :0,
        "camelcase" : 0,
        "eqeqeq" : 0
                     
    }
}